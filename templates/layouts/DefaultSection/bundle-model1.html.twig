{% if content %}
	{% set link  %}
	{{ content.link }}
	{% endset %}
	<div {{ attributes.addClass(['lesroisdelareno-bundle-model1','services-box-2','mb-5']) }}>
		<div class="content">
			<div class="b-mgr20">
				<a href="{{ link | striptags }}">
					{{ content.image }}</a>
			</div>
			<h4 class="titre">
				<a href="{{ link | striptags }}">{{ content.titre }}</a>
			</h4>
			{% set descript  %}
			{{ content.text }}
			{% endset %}
			{{ descript }}

			<div class="t-mgr10 d-none">
				<a href="{{ link | striptags }}">Lire la suite ...</a>
			</div>
		</div>
	</div>
{% endif %}
